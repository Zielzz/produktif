	<queryString>
		<![CDATA[SELECT psn.id_pesanan, psn.tanggal_pesanan, plg.nama_pelanggan, psn.merk_mobil, psn.nomor_plat, psn.total_harga, psn.status, lyn.nama_layanan, psn.is_delete
FROM pesanan psn
INNER JOIN pelanggan plg ON plg.id_pelanggan = psn.id_pelanggan
INNER JOIN layanan lyn ON lyn.id_layanan = psn.id_layanan
WHERE psn.is_delete = FALSE 
	AND psn.tanggal_pesanan BETWEEN $P{startDate} AND $P{endDate}]]>
	</queryString>
