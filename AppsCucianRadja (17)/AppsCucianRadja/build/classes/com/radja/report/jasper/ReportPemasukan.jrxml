	<queryString language="SQL">
		<![CDATA[SELECT 
    'Pemasukan' AS jenis,
    p.tanggal_pesanan AS tanggal,
    p.id_pesanan AS id_transaksi,
    p.merk_mobil,
    p.nomor_plat,
    l.nama_layanan,
    p.total_harga AS jumlah,
    p.insert_at AS waktu_transaksi
FROM 
    pesanan p
JOIN 
    layanan l ON p.id_layanan = l.id_layanan
WHERE 
    p.status = 'Selesai' 
    AND p.is_delete = 0;]]>
	</queryString>
